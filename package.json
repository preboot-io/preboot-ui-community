    "@mantine/modals": "^8.0.0",
    "@mantine/notifications": "^8.0.0",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "date-fns": "^4.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "typescript": "^5.7.2",
    "vite": "6.2.6",
    "vite-plugin-dts": "^4.4.0"
  },
  "dependencies": {
    "file-saver": "^2.0.5"
  }
